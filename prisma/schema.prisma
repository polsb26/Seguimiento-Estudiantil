generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model datosAlumno{
  id Int @id @default(autoincrement())
  DNI Int
  estado_Civil String
  sexo String
  edad Int
  fecha_Nacimiento DateTime @db.Date
  correo String
  celular String
  direccion String
  departamento String
  provincia String
  distrito String
}
